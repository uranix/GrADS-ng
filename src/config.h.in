/* Define if building universal (internal helper macro) */
#cmakedefine AC_APPLE_UNIVERSAL_BUILD

/* little_endian machine if BYTEORDER=0 */
#define BYTEORDER ${BYTEORDER}

/* Enabled geotiff */
#cmakedefine01 USEGEOTIFF
#define GEOTIFF USEGEOTIFF

/* Define if GetFile has a short prototype */
#cmakedefine GETFILE_SHORT_PROTOTYPE

/* Machine is a Cray */
#cmakedefine GRADS_CRAY

/* Machine is 64-bit HP */
#cmakedefine GRADS_HP64

/* GrADS version */
#define GRADS_VERSION "${CMAKE_PROJECT_VERSION}"

/* Enable grib2 */
#cmakedefine01 USEGRIB2
#define GRIB2 USEGRIB2

/* Enable netcdf4 */
#cmakedefine HAVENETCDF4

/* Define to 1 if fseeko (and presumably ftello) exists and is declared. */
#cmakedefine HAVE_FSEEKO

/* Define to 1 if you have the <malloc.h> header file. */
#cmakedefine HAVE_MALLOC_H

/* Define to 1 if you have the `SimpleGetFile' function. */
#cmakedefine HAVE_SIMPLEGETFILE

/* A macro that append sd_ to netcdf symbols if needed */
#cmakedefine HDF_NETCDF_NAME

/* Define to the sub-directory where libtool stores uninstalled libraries. */
#cmakedefine LT_OBJDIR

/* Define to the address where bug reports for this package should be sent. */
#cmakedefine PACKAGE_BUGREPORT

/* Define to the full name of this package. */
#cmakedefine PACKAGE_NAME

/* Define to the full name and version of this package. */
#cmakedefine PACKAGE_STRING

/* Define to the one symbol short name of this package. */
#cmakedefine PACKAGE_TARNAME

/* Define to the home page for this package. */
#cmakedefine PACKAGE_URL

/* Define to the version of this package. */
#cmakedefine PACKAGE_VERSION

/* Enabled command line editing */
#cmakedefine01 USEREADLINE
#define READLINE USEREADLINE

/* Define to 1 if you have the ANSI C header files. */
#cmakedefine STDC_HEADERS

/* Enable cairo */
#cmakedefine01 USECAIRO

/* Enable OPeNDAP for grids */
#cmakedefine01 USEDAP

/* Use GetFile from freq */
#cmakedefine01 USEFREQ

/* Enable OPeNDAP for station data */
#cmakedefine01 USEGADAP

/* Enable gd */
#cmakedefine01 USEGD

/* Enable GUI widgets */
#cmakedefine01 USEGUI

/* Enable hdf4 */
#cmakedefine01 USEHDF

/* Enable hdf5 */
#cmakedefine01 USEHDF5

/* Enable netcdf */
#cmakedefine01 USENETCDF

/* Enable shapefile */
#cmakedefine01 USESHP

/* Define WORDS_BIGENDIAN to 1 if your processor stores words with the most
   significant byte first (like Motorola and SPARC, unlike Intel). */
#if defined AC_APPLE_UNIVERSAL_BUILD
# if defined __BIG_ENDIAN__
#  define WORDS_BIGENDIAN 1
# endif
#else
# ifndef WORDS_BIGENDIAN
#  undef WORDS_BIGENDIAN
# endif
#endif

/* Define to 1 if the X Window System is missing or not being used. */
#cmakedefine X_DISPLAY_MISSING

/* Enable large inode numbers on Mac OS X 10.5.  */
#ifndef _DARWIN_USE_64_BIT_INODE
# define _DARWIN_USE_64_BIT_INODE 1
#endif

/* Number of bits in a file offset, on hosts where this is settable. */
#undef _FILE_OFFSET_BITS

/* Define to 1 to make fseeko visible on some hosts (e.g. glibc 2.2). */
#undef _LARGEFILE_SOURCE

/* Define for large files, on AIX-style hosts. */
#undef _LARGE_FILES

/* Define to empty if `const' does not conform to ANSI C. */
#undef const

/* Define to `long int' if <sys/types.h> does not define. */
#undef off_t
